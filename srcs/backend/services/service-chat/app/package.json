{
  "name": "test-fastify",
  "version": "1.0.0",
  "main": "srcs/api.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --clear --transpile-only srcs/api.ts",
    "build": "tsc",
    "start": "node dist/srcs/api.js",
    "test": "vitest --environment node --globals --include 'tests/**/*.test.ts'",
    "test:watch": "vitest --environment node --globals --include 'tests/**/*.test.ts' --watch",
    "prisma:generate": "prisma generate --schema=./srcs/prisma/prisma/schema.prisma",
    "prisma:migrate": "prisma migrate dev --schema=./srcs/prisma/prisma/schema.prisma --name initUserTable",
    "prisma:reset": "prisma migrate reset --force --schema=./srcs/prisma/prisma/schema.prisma",
    "prisma:studio": "prisma studio --schema=./srcs/prisma/prisma/schema.prisma"
  },
